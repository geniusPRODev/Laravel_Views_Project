{
    "name": "laravel-views/laravel-views",
    "description": "Laravel package to create beautiful common views like tables using only PHP code",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Martinez",
            "email": "gustavinho.martinez@gmail.com"
        }
    ],
    "require": {
        "livewire/livewire": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Gustavinho\\LaravelViews\\": "src",
            "Gustavinho\\LaravelViews\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*|8.5.*",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|4.*|5.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gustavinho\\LaravelViews\\LaravelViewsServiceProvider"
            ],
            "aliases": {
                "LaravelViews": "Gustavinho\\LaravelViews\\Facades\\LaravelViews",
                "Variants": "Gustavinho\\LaravelViews\\Facades\\Variants",
                "UI": "Gustavinho\\LaravelViews\\Facades\\UI"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
